    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "cross-env": "^3.1.4",
    "dayjs": "^1.8.26",
    "express": "^4.17.1",
    "express-validation": "^3.0.2",
    "express-ws": "^4.0.0",
    "jsonfile": "^6.0.1",
    "lodash": "^4.17.15",
    "mocha": "^7.1.2",
    "mysql": "^2.18.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
