{
  "name": "cryptocurrency",
  "version": "1.0.0",
  "description": "API for cryptocurrencies",
  "private": true,
  "main": "dist/src",
  "engines": {
    "node": ">=8.x"
  },
  "scripts": {
    "dev": "nodemon",
    "dev:inspect": "nodemon --exec \"node --inspect -r ts-node/register src\"",
    "build": "yarn run -s build:code",
